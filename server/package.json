{
  "scripts": {
    "build": "NODE_ENV=test tsc",
    "build-prod": "NODE_ENV=production tsc",
    "db-migrate": "cd ./src/db && tsc && cd ./../../db-build && sequelize db:migrate && cd ../ && rimraf ./db-build",
    "db-migrate:undo": "cd ./src/db && tsc && cd ./../../db-build && sequelize db:migrate:undo",
    "db-migrate:undo:all": "cd ./src/db && tsc && cd ./../../db-build && sequelize db:migrate:undo:all",
    "db-seed": "cd ./src/db && tsc && cd ./../../db-build && sequelize db:seed:all && cd ../ && rimraf ./db-build",
    "db-seed:undo": "cd ./src/db && tsc && cd ./../../db-build && sequelize db:seed:undo && cd ../ && rimraf ./db-build",
    "db-seed:undo:all": "cd ./src/db && tsc && cd ./../../db-build && sequelize db:seed:undo:all && cd ../ && rimraf ./db-build",
    "linter": "eslint '**/*.{ts,js,css}'",
    "prettier": "prettier --write '**/*.{ts,js,css,html,json}' && npx sort-package-json",
    "run-dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "run-prod": "NODE_ENV=production tsc && node ./build/src/server.js",
    "run-test": "NODE_ENV=test tsc && pm2-runtime start pm2.yaml",
    "test": "NODE_ENV=test jest --runInBand",
    "update-all": "npx npm-check-updates -u"
  },
  "dependencies": {
