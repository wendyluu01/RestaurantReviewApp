{
  "scripts": {
    "build": "NODE_ENV=test tsc",
    "build-prod": "NODE_ENV=production tsc",
    "db-migrate": "cd ./src/db && tsc && cd ./../../build/src/db && sequelize db:migrate && cd ../../../ && rimraf ./build/src/db",
    "db-migrate:undo": "cd ./src/db && tsc && cd ./../../build/src/db && sequelize db:migrate:undo",
    "db-migrate:undo:all": "cd ./src/db && tsc && cd ./../../build/src/db && sequelize db:migrate:undo:all",
    "db-seed": "cd ./src/db && tsc && cd ./../../build/src/db && sequelize db:seed:all && cd ../../../ && rimraf ./build/src/db",
    "db-seed:undo": "cd ./src/db && tsc && cd ./../../build/src/db && sequelize db:seed:undo && cd ../../../ && rimraf ./build/src/db",
    "db-seed:undo:all": "cd ./src/db && tsc && cd ./../../build/src/db && sequelize db:seed:undo:all && cd ../../../ && rimraf ./build/src/db",
    "linter": "tslint '**/*.{ts,js,css}'",
    "prettier": "prettier --write '**/*.{ts,js,css,html,json}' && npx sort-package-json",
    "run-dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "run-prod": "NODE_ENV=production tsc && node ./build/src/server.js",
    "run-test": "NODE_ENV=test tsc && pm2-runtime start pm2.yaml",
    "test": "NODE_ENV=test jest --runInBand",
    "update-all": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@aws-sdk/types": "^3.40.0",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.24",
    "@types/node-schedule": "^1.3.0",
    "@types/sequelize": "^4.28.9",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.10",
    "aws-sdk": ">= 2.1046.0",
    "axios": "^1.8.4",
    "b64id": "^1.0.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "4.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "2.8.5",
    "dotenv": "^8.2.0",
    "errorhandler": "1.5.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-status-monitor": "^1.2.6",
    "JSONStream": "^1.3.5",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.19",
    "lusca": "1.6.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.0",
    "node-schedule": "^2.1.1",
    "node-ses": "^0.0.1",
    "nodemailer": "^6.6.3",
    "path": "0.12.7",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.12.5",
    "sequelize-cli": "^6.3.0",
    "short-uuid": "^4.2.0",
    "socket.io": "^4.4.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "faker": "^4.1.0",
