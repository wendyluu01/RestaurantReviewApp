    "@types/node": "^20.8.0",
    "@types/node-schedule": "^2.0.0",
    "@types/sequelize": "^4.28.14",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "aws-sdk": "^2.1376.0",
    "axios": "^1.5.0",
    "b64id": "^1.0.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "chalk": "^5.3.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
